# Application Settings (Development)
APP_NAME=MOLIP-AI-Planner
DEBUG=True
HOST=0.0.0.0
PORT=8000

# Environment
ENVIRONMENT=development

# Backend URL
BACKEND_URL=https://stg.molip.today

# CORS Settings (개발 시 localhost도 허용)
CORS_ORIGINS=https://stg.molip.today,http://localhost:8000


# Gemini API Key 
